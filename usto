#include <stdio.h>

int main() {
    int i, j;

    printf("Multiplication Table from 1 to 10:\n\n");

    for (i = 1; i <= 10; i++) {
        for (j = 1; j <= 10; j++) {
            printf("%4d", i * j);
        }
        printf("\n"); // Move to the next line after each row
    }

    return 0;
}
